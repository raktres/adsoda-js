{
  "name": "adsoda-js",
  "version": "0.0.0",
  "description": "ADSODA after a Greg Ferrar idea",
  "main": "build/index.js",
  "scripts": {
    "test": "jest",
    "clean": "prettier --tab-width=4 --write ./src/*.js",
    "build": "webpack",
    "doc": "jsdoc  ./package.json  ./src/*.js",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raktres/adsoda-js"
  },
  "keywords": [
    "adsoda",
    "4DStroll",
    "4D"
  ],
  "author": "Jeff Bigot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/raktres/adsoda-js/issues"
  },
  "homepage": {
    "url": "https://github.com/raktres/adsoda-js#readme"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.11.1",
    "jest": "^23.6.0",
    "jsdoc": "^3.5.5",
    "lodash-webpack-plugin": "^0.11.5",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testEnvironment": "node"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
